{
  "name": "func-iter",
  "version": "0.0.1",
  "description": "Functional iterators for JS/TS",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --no-timeouts --colors -r ts-node/register tests/**.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glebbash/func-iter.git"
  },
  "keywords": [
    "js",
    "javascript",
    "ts",
    "typescript",
    "iterator",
    "functional"
  ],
  "author": "glebbash",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/glebbash/func-iter/issues"
  },
  "homepage": "https://github.com/glebbash/func-iter#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.0.0",
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "multiline-str": "^1.0.0",
    "nyc": "^15.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 4,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "commonjs": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "semi": [
        "error",
        "never"
      ]
    }
  }
}
